LoadPlugin mysql
<Plugin mysql>
  <Database "localhost">
    Host "localhost"
    User "<%= scope.lookupvar('collectd::plugin::mysql::mysql_username') %>"
    Password "<%= scope.lookupvar('collectd::plugin::mysql::mysql_password') %>"
    Socket "<%= scope.lookupvar('collectd::plugin::mysql::mysql_socket') %>"
    #MasterStats true
    #SlaveStats true
    #SlaveNotifications true
  </Database>
</Plugin>
